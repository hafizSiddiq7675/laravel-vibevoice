{
    "name": "bitsoftsol/laravel-vibevoice",
    "description": "Laravel package for Microsoft VibeVoice AI voice synthesis integration",
    "keywords": [
        "laravel",
        "vibevoice",
        "tts",
        "text-to-speech",
        "voice-synthesis",
        "ai",
        "audio"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "BitsoftSol Development Team",
            "email": "dev@bitsoftsol.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/queue": "^10.0|^11.0|^12.0",
        "illuminate/events": "^10.0|^11.0|^12.0",
        "illuminate/filesystem": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "BitsoftSol\\VibeVoice\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BitsoftSol\\VibeVoice\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BitsoftSol\\VibeVoice\\VibeVoiceServiceProvider"
            ],
            "aliases": {
                "VibeVoice": "BitsoftSol\\VibeVoice\\Facades\\VibeVoice"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
